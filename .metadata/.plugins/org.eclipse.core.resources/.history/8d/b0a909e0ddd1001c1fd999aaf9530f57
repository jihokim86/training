package problem1_4;

public class HeightWeight {
 public static void mian(String[] args) {
	 //신장별 표준 체중 대응표를 표시하는 프로그램을 작성
	 // 표시할 신장의 범위(시작값/종료값/증가값)는 정수로 입력받을것
	 // 표준체중(신장-100) x 0.9
	 
	 Scanner input = new Scanner(System.in);
	 int a= input.nextInt();
	 System.out.println("몇 cm부터 :"+a);
	 int b= input.nextInt();
	 System.out.println("몇 cm까지 :"+b);
	 int c= input.nextInt();
	 System.out.println("몇 cm 단위 :"+c);
	 
	 System.out.println("--------------");
 }
}
