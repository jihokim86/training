package problem1_4;

import java.util.Scanner;

public class Factorial {
	public static void main(String[] args) {
		 //양의 정수 값을 읽어서 1부터 n까지의 곱을 구하는 프로그램을 작성
		 
		 Scanner input = new Scanner(System.in);
		 
		 int a = input.nextInt();
		 int b=1;
		 
		 for(int i=1;i<=a;i++) {
			 b=i*b;
		 }
		 System.out.println(b);
		 input.close();
	 }

}
